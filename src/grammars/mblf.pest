operator = @{ "operator" } // TODO
operand = @{ "operand" }  // TODO

instruction = { operator ~ (" "+ ~ operand)*}

loopBlockStart = {  "[" }
loopBlockEnd = { "]" }
loopBlock = { loopBlockStart
            ~ instruction*
            ~ loopBlockEnd
}

statement = _{ instruction | loopBlock }

file = {SOI ~ (statement ~ ("\n" | "\r\n")+)* ~ EOI}
